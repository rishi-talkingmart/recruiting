exOf(query.toLowerCase()) > -1,\n    )\n    return exactEle.concat(containEle).slice(0, props.searchLimit)\n  }\n\n  const searchQuery = ref('')\n  const resetsearchQuery = () => {\n    searchQuery.value = ''\n  }\n\n  const handleSearchQueryUpdate = val => {\n    if (val === '') {\n      filteredData.value = {}\n    } else {\n      const queriedData = {}\n      const dataGrps = Object.keys(props.data)\n\n      dataGrps.forEach((grp, i) => {\n        queriedData[dataGrps[i]] = filterGrp(props.data[grp], val)\n      })\n\n      filteredData.value = queriedData\n    }\n  }\n\n  watch(searchQuery, val => handleSearchQueryUpdate(val))\n\n  return {\n    searchQuery,\n    resetsearchQuery,\n    filteredData,\n  }\n}\n"],"mappings":";;;;;;;;;AAAA,SAASA,GAAT,EAAcC,KAAd,QAA2B,sBAA3B;AAEA,eAAe,SAASC,cAAT,CAAwBC,KAAxB,EAA+B;EAC5C,IAAMC,YAAY,GAAGJ,GAAG,CAAC,EAAD,CAAxB;EAEA;AACF;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;EACE,IAAMK,SAAS,GAAG,SAAZA,SAAY,CAACC,GAAD,EAAMC,KAAN,EAAgB;IAChC,IAAMC,QAAQ,GAAGF,GAAG,CAACG,IAAJ,CAASC,MAAT,CAAgB,UAAAC,IAAI;MAAA,OAAIA,IAAI,CAACL,GAAG,CAACM,GAAL,CAAJ,CAAcC,WAAd,GAA4BC,UAA5B,CAAuCP,KAAK,CAACM,WAAN,EAAvC,CAAJ;IAAA,CAApB,CAAjB;IACA,IAAME,UAAU,GAAGT,GAAG,CAACG,IAAJ,CAASC,MAAT,EACjB;IACA,UAAAC,IAAI;MAAA,OAAI,CAACA,IAAI,CAACL,GAAG,CAACM,GAAL,CAAJ,CAAcC,WAAd,GAA4BC,UAA5B,CAAuCP,KAAK,CAACM,WAAN,EAAvC,CAAD,IAAgEF,IAAI,CAACL,GAAG,CAACM,GAAL,CAAJ,CAAcC,WAAd,GAA4BG,OAA5B,CAAoCT,KAAK,CAACM,WAAN,EAApC,IAA2D,CAAC,CAAhI;IAAA,CAFa,CAAnB;IAIA,OAAOL,QAAQ,CAACS,MAAT,CAAgBF,UAAhB,EAA4BG,KAA5B,CAAkC,CAAlC,EAAqCf,KAAK,CAACgB,WAA3C,CAAP;EACD,CAPD;;EASA,IAAMC,WAAW,GAAGpB,GAAG,CAAC,EAAD,CAAvB;;EACA,IAAMqB,gBAAgB,GAAG,SAAnBA,gBAAmB,GAAM;IAC7BD,WAAW,CAACE,KAAZ,GAAoB,EAApB;EACD,CAFD;;EAIA,IAAMC,uBAAuB,GAAG,SAA1BA,uBAA0B,CAAAC,GAAG,EAAI;IACrC,IAAIA,GAAG,KAAK,EAAZ,EAAgB;MACdpB,YAAY,CAACkB,KAAb,GAAqB,EAArB;IACD,CAFD,MAEO;MACL,IAAMG,WAAW,GAAG,EAApB;MACA,IAAMC,QAAQ,GAAGC,MAAM,CAACC,IAAP,CAAYzB,KAAK,CAACM,IAAlB,CAAjB;MAEAiB,QAAQ,CAACG,OAAT,CAAiB,UAACvB,GAAD,EAAM